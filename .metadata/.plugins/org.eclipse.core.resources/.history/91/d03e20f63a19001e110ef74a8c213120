package ex_01;

public class Main {

	public static void main(String[] args) {
		//정수 형 :: 정수는 2남 막내
		int num1=80;	//형 하나
		
		//실수 형 :: 실수는 3남 막내
		float num2=80.5f;	//첫째
		double num3=81.6;	//둘째
		
		//문자 형 :: 문자는 3남 막내
		char str = 'a';	//첫째
		String str2 = "abc";//둘째
		
		
		System.out.printf("%d,%f,%.2f",num1,num2,num3);
		
		
	}
	//인덴트 (indent) :: Tab눌러서 하는거
}
