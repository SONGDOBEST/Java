package day09;

public class FruitMain {

	public static void main(String[] args) {
		
		Fruit apple = new Fruit();
		apple.name = "사과";
		apple.color = "빨강";
		apple.isSeed = true;
		
		
		
				
		Fruit banana = new Fruit("바나나","노랑",false);
		
		
		apple.print();
		banana.print();
		
		Fruit melon = new Fruit("멜론", "녹색");
		
		melon.isSeed = true;
		melon.print();
		
		Fruit orange = new Fruit("오렌지","주황색", false)
		
		
		
		

	}

}
